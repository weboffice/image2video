{
  "name": "image2video",
  "version": "1.0.0",
  "description": "Sistema de conversão de imagens para vídeo",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python main.py",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "build": "cd frontend && npm run build",
    "start": "cd backend && python main.py",
    "test:integration": "./scripts/test-integration.sh"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["image", "video", "conversion", "fastapi", "react"],
  "author": "",
  "license": "MIT"
}
